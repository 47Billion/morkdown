{
    "name"         : "morkdown"
  , "version"      : "1.0.1"
  , "description"  : "A markdown editor built on Chrome & Node"
  , "main"         : "lib/server.js"
  , "dependencies" : {
          "delayed"       : "~0.0.0"
        , "concat-stream" : "~0.1.1"
        , "domready"      : "~0.2.11"
        , "hyperquest"    : "~0.1.5"
        , "browserify"    : "~2.13.2"
        , "brucedown"     : "~0.0.1"
