{
    "name"         : "morkdown"
  , "version"      : "2.0.0"
  , "description"  : "A markdown editor built on Chrome & Node"
  , "main"         : "lib/server.js"
  , "dependencies" : {
          "delayed"       : "~0.0.0"
        , "concat-stream" : "~0.1.1"
        , "domready"      : "~0.2.11"
        , "hyperquest"    : "~0.1.5"
        , "browserify"    : "~2.13.2"
        , "brucedown"     : "~0.0.1"
        , "after"         : "~0.7.0"
        , "st"            : "~0.1.3"
        , "ent"           : "~0.0.5"
        , "codemirror"    : "~3.13.00"
        , "optimist"      : "~0.5.0"
      }
  , "bin"          : {
        "morkdown"        : "./bin/morkdown.js"
    }
  , "keywords"     : [
        "markdown"
    ]
  , "repository"   : {
        "type"            : "git"
      , "url"             : "https://github.com/rvagg/morkdown.git"
    }
  , "preferGlobal" : true
  , "authors"      : [
        "Rod Vagg <rod@vagg.org> (https://github.com/rvagg)"
    ]
  , "scripts"      : {
        "install" : "mkdir -p static/codemirror/; cp -a node_modules/codemirror/lib/codemirror.css node_modules/codemirror/theme/ static/codemirror/"
    }
  , "license"      : "MIT"
}